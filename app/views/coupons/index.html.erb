<h1>My Coupons</h1>
    <center>
      <h2><%= link_to "Create New Coupon", new_merchant_coupon_path(@merchant) %></h2>
    </center>
<div class="columns-container">
  <div class="column">
    <h2>Active Coupons</h2>
    <% @active_coupons.each do |coupon| %>
      <p>
        <h2><%= link_to coupon.name, merchant_coupon_path(@merchant, coupon) %></h2>
      </p>
    <% end %>
  </div>
  <div class="column">
    <h2>Inactive Coupons</h2>
    <% @inactive_coupons.each do |coupon| %>
      <p>
        <h2><%= link_to coupon.name, merchant_coupon_path(@merchant, coupon) %></h2>
      </p>
    <% end %>
  </div>
</div>